{
  "name": "curso-de-webpack",
  "version": "1.0.0",
  "description": "este es un cruso muy divertido",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js --output bundle.js --mode development",
    "build:local": "webpack --mode development",
    "build:external": "webpack --config ./external/webpack.config.js --mode development",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js --mode development",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js --mode development",
    "build:multi": "webpack --config ./multiple-entry-point/webpack.config.js --mode development",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js --mode development",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js --mode development",
    "build:images": "webpack --config ./url-loader-images/webpack.config.js --mode development",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js --mode development",
    "build:video": "webpack --config ./url-loader-video/webpack.config.js --mode development",
    "build:json": "webpack --config ./json-loader/webpack.config.js --mode development",
    "build:react": "webpack --config ./react/webpack.config.js --mode development"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Luis Diaz <luisdiazvenero@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.5.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  }
}
